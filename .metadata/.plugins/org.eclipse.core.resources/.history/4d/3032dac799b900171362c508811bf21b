package ie.gmit.sw;


import java.util.*;

public class TypeC {
	
	private List<TypeD> list = new ArrayList<TypeD>();
	private Date date = new Date();
	/* (non-Javadoc)
	 * @see java.lang.Object#finalize()
	 */
	@Override
	protected void finalize() throws Throwable {
		// TODO Auto-generated method stub
		System.out.println("Type C about to be GC"+this);
	}

	public TypeC(Date d) {
		this.date = d;
	}
	
	public void add(TypeD d) {
		this.list.add(d);
	}
	public void delete(TypeD d) {
		this.list.remove(d);
	}
	
	public int count() {
		return this.list.size();
	}
	
	public boolean contains(TypeD d) {
		return this.list.contains(d);
	}
	
	public List<TypeD> elements() {
		return this.list;
	}

}
